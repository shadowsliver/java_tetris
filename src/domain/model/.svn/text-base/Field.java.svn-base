package domain.model;

public class Field {
private int height;
private int width;
private String colorHex;

public Field (int height, int width, String colorHexadecimal) {
	this.setColorHex(colorHexadecimal);
	this.setHeight(height);
	this.setWidth(width);
}

public int getHeight() {
	return height;
}

public int getWidth() {
	return width;
}

public String getColorHex() {
	return colorHex;
}

public void setHeight(int height) {
	this.height = height;
}

public void setWidth(int width) {
	this.width = width;
}

private void setColorHex(String colorHex) {
	this.colorHex = colorHex;
}
	
	
	
	

}
